int i = 0;

void setup()
{
size(600,600);
background(255);
}
void draw()
{
float x = random(width);
float y = random(height);

stroke(0);
strokeWeight(3);
int r = int(random(200,255));
int g = int(random(0,100));
int b = int(random(100,200));

fill(r, g, b);
rect(x, y, random(25,75), random(25,75));
}
